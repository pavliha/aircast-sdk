{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist",
		"declaration": true,
		"rootDir": ".",
		"typeRoots": ["./node_modules/@types"],
		"lib": ["ES2020", "DOM"],
		"resolveJsonModule": true
	},
	"include": ["gen/**/*", "examples/**/*"],
	"exclude": ["node_modules", "dist"]
}
